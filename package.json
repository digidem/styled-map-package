    "readable-stream": "^4.7.0",
    "temp-dir": "^3.0.0",
    "yauzl-promise": "^4.0.0",
    "yocto-queue": "^1.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.1",
    "@jsquash/jpeg": "^1.4.0",
    "@jsquash/png": "^3.0.1",
    "@stealthybox/jpg-stream": "^1.1.2",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/archiver": "^6.0.3",
    "@types/geojson": "^7946.0.16",
    "@types/http-errors": "^2.0.4",
    "@types/mapbox__sphericalmercator": "^1.2.3",
    "@types/node": "^18.19.130",
    "@types/readable-stream": "^4.0.18",
    "@types/yauzl-promise": "^4.0.1",
    "ava": "^6.2.0",
    "block-stream2": "^2.1.0",
    "esbuild-fix-imports-plugin": "^1.0.20",
    "eslint": "^9.25.1",
    "execa": "^9.4.0",
    "globals": "^16.0.0",
    "husky": "^9.1.7",
    "jpg-stream": "^1.1.2",
    "lint-staged": "^15.4.3",
    "pixel-stream": "^1.0.3",
    "playwright": "^1.50.1",
    "png-stream": "^1.0.5",
    "prettier": "^3.5.2",
    "random-bytes-readable-stream": "^3.0.0",
    "tempy": "^3.1.0",
    "tsup": "^8.4.0",
    "type-fest": "^4.35.0",
    "typescript": "^5.9.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "importOrder": [
      "^node:",
      "^[./]"
    ],
    "importOrderSeparation": true,
    "plugins": [
      "@trivago/prettier-plugin-sort-imports"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --cache --fix"
    ],
    "*.{js,css,md}": "prettier --write"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digidem/styled-map-package.git"
  },
  "bugs": {
    "url": "https://github.com/digidem/styled-map-package/issues"
  }
}
