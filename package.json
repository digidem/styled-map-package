    "ansi-diff": "^1.2.0",
    "archiver": "^7.0.1",
    "buffer-peek-stream": "^1.1.0",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "fastify": "^5.6.2",
    "filter-obj": "^6.1.0",
    "into-stream": "^8.0.1",
    "is-stream": "^4.0.1",
    "itty-router": "^5.0.22",
    "ky": "^1.7.5",
    "log-symbols": "^7.0.0",
    "map-obj": "^5.0.2",
    "mbtiles-reader": "^1.0.0",
    "open": "^10.1.0",
    "ora": "^8.2.0",
    "p-event": "^6.0.1",
